<p id="notice"><%= notice %></p>

<h1>Listing Transactions</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Entity</th>
      <th>Is void</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transaktions.each do |transaktion| %>
      <tr>
        <td><%= transaktion.date %></td>
        <td><%= transaktion.entity %></td>
        <td><%= transaktion.is_void %></td>
        <td><%= link_to 'Show', transaktion %></td>
        <td><%= link_to 'Edit', edit_transaktion_path(transaktion) %></td>
        <td><%= link_to 'Destroy', transaktion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transaction', new_transaktion_path %>
